			          			<td>{{field}}</td>
			          			<td>{{def.picture}}</td>
			          		</tr>
			          		{% endfor %}
			          	</tbody>
			          </table>
			        </div>
			    </div>

			    <div class="panel panel-default">
			        <div class="panel-heading" role="tab" id="remessaHeadingSegmentos">
			          <h3 class="panel-title">
			            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#remessaSegmentos" aria-expanded="true" aria-controls="remessaSegmentos">
			              SEGMENTOS
			            </a>
			          </h3>
			        </div>
			        <div id="remessaSegmentos" class="panel-collapse collapse" role="tabpanel" aria-labelledby="remessaHeadingSegmentos">
						{% for segmento, def in layout.remessa.detalhes %}
							<h3 class="text-center" style="border-bottom: 1px solid #ddd">{{segmento|replace({'_': ' '})|upper}}</h3>
							<table class="table table-striped table-hover">
					          	<thead>
					          		<tr>
					          			<th>Campo</th>
					          			<th>Formato</th>
					          		</tr>
					          	</thead>
					          	<tbody>
					          		{% for field, deff in def %}
