<body>
	
	<header>
		<div class="container">
			<div class="page-header">
			  <h2>Servi√ßo: <small>{{layout.servico}}</small></h2>
			  <h4 style="margin: 0">Layout: <small>{{layout.layout}}</small></h4>
			</div>
		</div>
	</header>

	<section id="remessa">
		<div class="container">
			<h2>Remessa</h2>

			<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
			    <div class="panel panel-default">
			        <div class="panel-heading" role="tab" id="remessaHeadingHeaderArquivo">
			          <h3 class="panel-title">
			            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#remessaHeaderArquivo" aria-expanded="true" aria-controls="remessaHeaderArquivo">
			              HEADER ARQUIVO
			            </a>
			          </h3>
			        </div>
			        <div id="remessaHeaderArquivo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="remessaHeadingHeaderArquivo">
			          <table class="table table-striped table-hover">
			          	<thead>
			          		<tr>
			          			<th>Campo</th>
			          			<th>Formato</th>
			          		</tr>
			          	</thead>
			          	<tbody>
			          		{% for field, def in layout.remessa.header_arquivo %}
			          		<tr>
			          			<td>{{field}}</td>
			          			<td>{{def.picture}}</td>
			          		</tr>
			          		{% endfor %}
			          	</tbody>
			          </table>
			        </div>
			    </div>

			    <div class="panel panel-default">
			        <div class="panel-heading" role="tab" id="remessaHeadingHeaderLote">
			          <h3 class="panel-title">
			            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#remessaHeaderLote" aria-expanded="true" aria-controls="remessaHeaderLote">
